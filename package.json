{
  "name": "ngx-material-extra-workspace",
  "version": "19.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve --project demo-app",
    "build": "ng build --project demo-app",
    "watch:lib": "ng build ngx-material-extra --watch",
    "build:lib": "ng build ngx-material-extra",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "version:patch": "cd projects/ngx-material-extra && npm version patch",
    "version:minor": "cd projects/ngx-material-extra && npm version minor",
    "version:major": "cd projects/ngx-material-extra && npm version major",
    "publish:lib": "npm run build:lib && cd dist/ngx-material-extra && npm publish"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^19.2.10",
    "@angular/cdk": "^19.2.15",
